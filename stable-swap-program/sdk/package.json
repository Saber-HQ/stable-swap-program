  "engines": {
    "node": ">=12.x"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@saberhq/eslint-config": "^1.12.6",
    "@saberhq/solana-contrib": "^1.12.6",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.0.3",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/node": "^17.0.5",
    "b58": "^4.0.3",
    "eslint": "^8.5.0",
    "husky": ">=7.0.4",
    "jest": "^27.4.5",
    "lint-staged": ">=12.1.4",
    "lodash.mapvalues": "^4.6.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
